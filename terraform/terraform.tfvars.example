# Copy this file to terraform.tfvars and fill in your values

project_id      = "your-gcp-project-id"
region          = "us-central1"
environment     = "dev"

# Use Vertex AI for authentication (recommended for production)
use_vertex_ai = true

# Only needed if use_vertex_ai = false
# gemini_api_key = "your-api-key"

# Scaling
min_instances = 0
max_instances = 2

# Evaluation sampling rate
eval_sample_rate = 0.05

# GitHub for Cloud Build
github_owner = "your-user-name"
github_repo = "your-repo-name"